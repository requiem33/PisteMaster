{
  "name": "pistemaster-frontend",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "preview": "vite preview",
    "dev:fullstack": "concurrently \"npm run dev\" \"cd ../backend && python manage.py runserver\"",
    "lint": "eslint . --ext .vue,.js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "format": "prettier --write .",
    "type-check": "vue-tsc --noEmit",
    "prepare": "husky"
  },
  "dependencies": {
    "vue": "^3.4.0",
    "pinia": "^2.1.0",
    "vue-router": "^4.2.0",
    "element-plus": "^2.3.0",
    "@element-plus/icons-vue": "^2.3.0",
    "ag-grid-community": "^31.1.0",
    "ag-grid-vue3": "^31.1.0",
    "idb": "^8.0.0",
    "localforage": "^1.10.0",
    "vue-i18n": "^9.4.0",
    "axios": "^1.6.0",
    "date-fns": "^3.0.0",
    "uuid": "^9.0.0",
    "jszip": "^3.10.0",
    "print-js": "^1.6.0",
    "@vueuse/core": "^10.0.0",
    "vue-virtual-scroller": "^1.1.2"
  },
  "devDependencies": {
    "@vitejs/plugin-vue": "^5.0.0",
    "vite": "^5.0.0",
    "typescript": "^5.0.0",
    "vue-tsc": "^1.8.0",
    "@types/node": "^20.0.0",
    "@types/uuid": "^9.0.0",
    "sass": "^1.69.0",
    "sass-loader": "^14.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.1.0",
    "concurrently": "^8.0.0",
    "husky": "^8.0.0",
    "lint-staged": "^15.0.0"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,vue}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}